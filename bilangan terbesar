try:
  bil1 = int(input("Masukkan bilangan pertama: "))
  bil2 = int(input("Masukkan bilangan kedua: "))
  bil3 = int(input("Masukkan bilangan ketiga: "))
  if bil1 > bil2 and bil3 > bil3:
    print("Terbesar: ", bil1)
  elif bil2 > bil1 and bil2 > bil3:
    print("Terbesar: ", bil2)
  elif bil3 > bil1 and bil3 > bil2:
    print("Terbesar: ", bil3)
except:
    print("Anda harus memasukan bilangan bulat!!!!")
